<div>
    <nav>
        <button id="logout">logout</button>
        <a href="/cartView">Ver mi carrito</a>
        <a href="/profile"><img src="https://i.pinimg.com/236x/2f/97/f0/2f97f05b32547f54ef1bdf99cd207c90.jpg" alt="Foto de perfil"></a>
    </nav>
    <h1>
        Bienvenido a LyonProducts!
    </h1>

    <div>
        <h2>Vea Nuestro Productos</h2>
    <table>
        <tr>
            <th>ID</th>
            <th>TITLE</th>
            <th>DESCRIPTION</th>
            <th>PRICE</th>
            <th>CODE</th>
            <th>STATUS</th>
            <th>STOCK</th>
        </tr>
        <tbody>
            {{#each productList}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.code}}</td>
                <td>{{this.status}}</td>
                <td>{{this.category}}</td>
                <td>{{this.stock}}</td>
                <td><button class="addProduct" data-info={{this.id}}>Agregar producto al carrito</button><td>
            </tr>
            {{/each}}
        </tbody>
    </table>

</div>

    <script src="/js/main.js"></script>
</div>